<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@color/color_account_ok_down">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/transparent" />
        </shape>
    </item>
    <item android:state_pressed="false" android:drawable="@color/color_account_ok">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/transparent" />
        </shape>
    </item>
</selector>