<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="50.0dip"
        android:layout_marginLeft="5.0dip"
        android:layout_marginRight="5.0dip"
        android:background="@drawable/gr_train_bg" >

        <TextView
            android:id="@id/train_station"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center"
            android:layout_marginLeft="5.0dip"
            android:gravity="center"
            android:text="石家庄"
            android:textColor="@color/white"
            android:textSize="16.0sp" />

        <TextView
            android:id="@id/train_stand_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="07:00"
            android:textColor="@color/white"
            android:textSize="16.0sp" />

        <TextView
            android:id="@id/train_distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center"
            android:layout_marginRight="5.0dip"
            android:gravity="center"
            android:text="50km"
            android:textColor="@color/white"
            android:textSize="16.0sp" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10.0dip"
        android:layout_marginTop="50.0dip"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <FrameLayout
                android:id="@id/train_seat_ying"
                android:layout_width="0.0dip"
                android:layout_height="40.0dip"
                android:layout_weight="1.0" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:text="@string/train_seat_ying_1"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <TextView
                    android:id="@id/train_seat_ying_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="50.0dip"
                    android:gravity="center"
                    android:text="07:0哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈0"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />
            </FrameLayout>

            <FrameLayout
                android:id="@id/train_seat_ruan"
                android:layout_width="0.0dip"
                android:layout_height="40.0dip"
                android:layout_weight="1.0" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:text="@string/train_seat_ruan_1"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <TextView
                    android:id="@id/train_seat_ruan_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="50.0dip"
                    android:gravity="center"
                    android:text="07:00"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <FrameLayout
                android:id="@id/train_bed_ying"
                android:layout_width="0.0dip"
                android:layout_height="40.0dip"
                android:layout_weight="1.0" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:text="@string/train_bed_ying_1"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <TextView
                    android:id="@id/train_bed_ying_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="50.0dip"
                    android:gravity="center"
                    android:text="07:00"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />
            </FrameLayout>

            <FrameLayout
                android:id="@id/train_bed_ruan"
                android:layout_width="0.0dip"
                android:layout_height="40.0dip"
                android:layout_weight="1.0" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:text="@string/train_bed_ruan_1"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <TextView
                    android:id="@id/train_bed_ruan_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="50.0dip"
                    android:gravity="center"
                    android:text="07:00"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <FrameLayout
                android:id="@id/train_gao_one"
                android:layout_width="0.0dip"
                android:layout_height="40.0dip"
                android:layout_weight="1.0" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:text="@string/train_gao_one_1"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <TextView
                    android:id="@id/train_gao_one_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="65.0dip"
                    android:gravity="center"
                    android:text="07:00"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />
            </FrameLayout>

            <FrameLayout
                android:id="@id/train_gao_two"
                android:layout_width="0.0dip"
                android:layout_height="40.0dip"
                android:layout_weight="1.0" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="10.0dip"
                    android:gravity="center"
                    android:text="@string/train_gao_two_1"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <TextView
                    android:id="@id/train_gao_two_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="65.0dip"
                    android:gravity="center"
                    android:text="07:00"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>